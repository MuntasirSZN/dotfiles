{
  "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
"logo": {
    "source": "~/.config/fastfetch/archlogo.txt",
    // "type": "kitty-direct",
    "color": {
      "1": "blue",
      "2": "cyan",
    },
    "height": 15,
    "width": 30,
    "padding": {
      "top": 1,
    },
  },
  "display": {
    "separator": " -> ",
  },
  "modules": [
    "break",
    {
      "type": "title",
      "keyWidth": 10,
      "format": "         {6}{7}{8}",
    },
    {
      "type": "custom",
      "format": "  ╭───────────────────────╮",
    },
    {
      "type": "os",
      "key": " DISTRO",
      "keyColor": "31",
    },
    {
      "type": "kernel",
      "key": " ├  ",
      "keyColor": "31",
    },
    {
      "type": "packages",
      "key": " ├ 󰏖 ",
      "keyColor": "31",
    },
    {
      "type": "shell",
      "key": " └  ",
      "keyColor": "31",
    },
    "break",
    {
      "type": "wm",
      "key": " DE/WM",
      "keyColor": "32",
    },
    {
      "type": "wmtheme",
      "key": " ├ 󰉼 ",
      "keyColor": "32",
    },
    {
      "type": "icons",
      "key": " ├ 󰀻 ",
      "keyColor": "32",
    },
    {
      "type": "cursor",
      "key": " ├  ",
      "keyColor": "32",
    },
    {
      "type": "terminal",
      "key": " ├  ",
      "keyColor": "32",
    },
    {
      "type": "terminalfont",
      "key": " └  ",
      "keyColor": "32",
    },
    "break",
    {
      "type": "host",
      "format": "{2}",
      "key": "󰌢 SYSTEM",
      "keyColor": "33",
    },
    {
      "type": "cpu",
      "format": "{1} @ {7}",
      "key": " ├  ",
      "keyColor": "33",
    },
    {
      "type": "gpu",
      "format": "{2}",
      "key": " ├ 󰢮 ",
      "keyColor": "33",
    },
    {
      "type": "memory",
      "key": " ├  ",
      "keyColor": "33",
    },
    {
      "type": "swap",
      "key": " ├ 󰓡 ",
      "keyColor": "33",
    },
    {
      "type": "disk",
      "key": " ├ 󰋊 ",
      "keyColor": "33",
    },
    {
      "type": "display",
      "key": " └  ",
      "compactType": "original-with-refresh-rate",
      "keyColor": "33",
    },
    {
      "type": "custom",
      "format": "  ╰───────────────────────╯",
    },
    {
      "type": "custom",
      "format": "   \u001b[31m  \u001b[32m  \u001b[33m  \u001b[34m  \u001b[35m  \u001b[36m  \u001b[37m  \u001b[90m ",
    },
    "break",
  ],
}

